// Simplified Control Flow Graph (CFG)
digraph {
	N0 [label="Function: swap"]
	N4 [label="End of swap"]
	N5 [label="Function: partition"]
	N8 [label="for(int j = low; j <= (high - 1); j++)"]
	N12 [label=N12]
	N13 [label="exit for"]
	N15 [label="return i + 1"]
	N16 [label="End of partition"]
	N17 [label="Function: quickSort"]
	N23 [label="End of quickSort"]
	N0 -> N4
	N5 -> N8
	N8 -> N12
	N8 -> N13
	N12 -> N8
	N13 -> N15
	N17 -> N23
}
